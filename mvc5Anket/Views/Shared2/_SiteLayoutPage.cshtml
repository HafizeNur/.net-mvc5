<!DOCTYPE html>
<html lang="en">
<head>
    <title></title>
    <meta charset="utf-8">
    <meta name="format-detection" content="telephone=no" />
    <link href="~/Content/images/favicon.ico" rel="icon">
    <link href="~/Content/images/favicon.ico" rel="shortcut icon">
    <link href="~/Content/booking/css/booking.css" rel="stylesheet" />
    <link href="~/Content/css/camera.css" rel="stylesheet" />
    <link href="~/Content/css/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <link href="~/Content/css/superfish.css" rel="stylesheet" />
    <script src="~/Content/js/jquery.js"></script>
    <script src="~/Content/js/superfish.js"></script>
    <script src="~/Content/js/jquery-migrate-1.2.1.js"></script>
    <script src="~/Content/js/script.js"></script>
    <script src="~/Content/js/jquery.ui.totop.js"></script>
    <script src="~/Content/js/jquery.equalheights.js"></script>
    <script src="~/Content/js/jquery.mobilemenu.js"></script>
    <script src="~/Content/js/jquery.easing.1.3.js"></script>
    <script src="~/Content/js/camera.js"></script>
    <script src="~/Content/js/owl.carousel.js"></script>
    <script src="~/Content/js/jquery.mobile.customized.min.js"></script>
    <script src="~/Content/booking/js/booking.js"></script>
    <style type="text/css">
        .menu_block {
            border-radius: 0 0 100px 100px;
        }
            .bq1; {
            height: 20px;
        }

        .prefix_1 {
            height: 122px;
        }
    </style>
    <script>
        $(document).ready(function () {
            jQuery('#camera_wrap').camera({
                loader: false,
                pagination: false,
                minHeight: '444',
                thumbnails: false,
                height: '48.375%',
                caption: true,
                navigation: true,
                fx: 'mosaic'
            });
            /*carousel*/
            var owl = $("#owl");
            owl.owlCarousel({
                items: 2, //10 items above 1000px browser width
                itemsDesktop: [995, 2], //5 items between 1000px and 901px
                itemsDesktopSmall: [767, 2], // betweem 900px and 601px
                itemsTablet: [700, 2], //2 items between 600 and 0
                itemsMobile: [479, 1], // itemsMobile disabled - inherit from itemsTablet option
                navigation: true,
                pagination: false
            });
            $().UItoTop({ easingType: 'easeOutQuart' });
        });
    </script>
</head>
<body class="page1" id="top">
    <!--==============================header=================================-->
    <header>
        <div class="container_12">
            <div class="grid_12">
                <div class="menu_block">
                    <nav class="horizontal-nav full-width horizontalNav-notprocessed">
                        <ul class="sf-menu">
                         
                            <li><a href=""></a></li>
                            <li class="current"><a href="http://www.samsun.bel.tr/# ">Samsun BŞB</a></li>
                            <li><a href="/SiteHome/Index">Ana Sayfa</a></li>
                            @*<li><a href="">Hakkımızda</a></li>*@
                            <li><a href="/AllSurvey/Index">Tüm Anketler</a></li>
                            <li><a href=""></a></li>
                            @*<li><a href="index-3.html">İletişim</a></li>*@
                        </ul>
                    </nav>
                    <div class="clear"></div>
                </div>
            </div>
            <div class="grid_12">
                <h1>
                    <a href="index.html">
                        <img src="images/logo.png" alt="Your Happy Family">
                    </a>
                </h1>
            </div>
        </div>
    </header>

    <div class="slider_wrapper">
        <div id="camera_wrap" class="">
            @if (ViewBag.LastThreeSlider != null)
            {
                <div data-src="/Areas/admin/Content/Site/images/slider/@ViewBag.LastThreeSlider[0].ImagePath">
                    <div class="caption fadeIn">
                        <h2> @ViewBag.LastThreeSlider[0].Title</h2>
                        @*<a href="#">Tarihçemiz</a>*@
                    </div>
                </div>
                if (ViewBag.LastThreeSlider.Count > 1)
                {
                    <div data-src="/Areas/admin/Content/Site/images/slider/@ViewBag.LastThreeSlider[1].ImagePath">
                        <div class="caption fadeIn">
                            <h2> @ViewBag.LastThreeSlider[1].Title</h2>
                        </div>
                    </div>
                }

                if (ViewBag.LastThreeSlider.Count > 2)
                {
                    <div data-src="/Areas/admin/Content/Site/images/slider/@ViewBag.LastThreeSlider[2].ImagePath">
                        <div class="caption fadeIn">
                            <h2>@ViewBag.LastThreeSlider[2].Title</h2>
                        </div>
                    </div>
                }
            }
        </div>

    </div>
    <!--==============================Content=================================-->
    <div class="content">
        <div class="ic">More Website Templates  TemplateMonster.com - February 10, 2014!</div>
        <div class="container_12">
            @if (ViewBag.LastThreeSurvey != null)
            {
                <div class="grid_4">
                    <div class="banner">
                        <img src="~/Areas/admin/Content/Site/images/addsurvey/@ViewBag.LastThreeSurvey[0].ImagePath" alt="">
                        <div class="label">
                            <div class="title">@ViewBag.LastThreeSurvey[0].SurveyName</div>
                            <a href="/SiteSurvey/Index?surveyid=@ViewBag.LastThreeSurvey[0].ID">ANKETE GİT</a>
                        </div>
                    </div>
                </div>
                if (ViewBag.LastThreeSurvey.Count > 1)
                {
                    <div class="grid_4">
                        <div class="banner">
                            <img src="~/Areas/admin/Content/Site/images/addsurvey/@ViewBag.LastThreeSurvey[1].ImagePath" alt="">
                            <div class="label">
                                <div class="title">@ViewBag.LastThreeSurvey[1].SurveyName</div>
                                <a href="/SiteSurvey/Index?surveyid=@ViewBag.LastThreeSurvey[1].ID">ANKETE GİT</a>
                            </div>
                        </div>
                    </div>
                }
                if (ViewBag.LastThreeSurvey.Count > 2)
                {
                    <div class="grid_4">
                        <div class="banner">
                            <img src="~/Areas/admin/Content/Site/images/addsurvey/@ViewBag.LastThreeSurvey[2].ImagePath" alt="">
                            <div class="label">
                                <div class="title">@ViewBag.LastThreeSurvey[2].SurveyName</div>
                                <a href="/SiteSurvey/Index?surveyid=@ViewBag.LastThreeSurvey[2].ID">ANKETE GİT</a>
                            </div>
                        </div>
                    </div>
                }
            }
        </div>
        <div>
            

                @RenderBody()
</div>
    </div>

    <!--==============================footer=================================-->
    <footer>
        <div class="container_12">
            <div class="grid_12">
                <div class="socials">
                    <a href="https://www.facebook.com/SamsunBuyuksehir" class="fa fa-facebook"></a>
                    <a href="https://twitter.com/samsunbsb" class="fa fa-twitter"></a>
                    @*<a href="#" class="fa fa-google-plus"></a>*@
                </div>
                <div class="copy">
                    Samsun Büyükşehir Belediyesi 2016 | <a href="#">Privacy Policy</a> | Website Template Designed by <a href="http://www.samsun.bel.tr/#" rel="nofollow">hafizenurkorkmaz@gmail.com</a>
                </div>
            </div>
        </div>
    </footer>
